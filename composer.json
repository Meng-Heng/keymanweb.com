{
    "name": "keyman/keymanweb.com",
    "require": {
        "sentry/sdk": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit --testdox",
        "check-links": [
          "Composer\\Config::disableProcessTimeout",
          "npx broken-link-checker http://keymanweb.com.local --ordered --recursive --host-requests 10 -e --filter-level 3"
        ],
        "check-docker-links": [
          "Composer\\Config::disableProcessTimeout",
          "npx broken-link-checker http://localhost:8057 --ordered --recursive --host-requests 10 -e --filter-level 3"
        ],
        "lint": "find . -name '*.php' | grep -v '/vendor/' | xargs -n 1 -d '\\n' php -l"
    }
}
